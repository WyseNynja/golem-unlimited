[Unit]                                                                                                                                                                          
Description=gu-provider
After=network.target

[Service]
User=golemu
ExecStart=/usr/bin/gu-provider server run

LimitNOFILE=150000
StandardOutput=syslog+console
Restart=always
RestartSec=3

[Install]
WantedBy=multi-user.target

